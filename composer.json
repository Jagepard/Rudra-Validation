{
    "name": "rudra/validation",
    "description": "Rudra framework",
    "type": "library",
    "keywords": [
        "rudra",
        "validation",
        "validation-library",
        "validator"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jagepard",
            "email": "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jagepard@yandex.ru",
        "issues": "https://github.com/Jagepard/Rudra-Validation/issues",    
        "source": "https://github.com/Jagepard/Rudra-Validation"    
    },
    "require": {
        "php": ">=8.3",
        "rudra/container": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Rudra\\Validation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rudra\\Validation\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
