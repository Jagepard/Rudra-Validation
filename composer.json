{
    "name"        : "rudra/validation",
    "description" : "The Rudra framework",
    "type"        : "library",
    "keywords"    : [
        "rudra",
        "validation",
        "validation-library",
        "validator"
    ],
    "license"     : "MIT",
    "authors"     : [
        {
            "name"    : "Danila Korotkov (Jagepard)",
            "email"   : "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role"    : "Developer"
        }
    ],
    "support"     : {
        "email": "jagepard@yandex.ru",
        "skype": "jagepard.ru"
    },
    "require"     : {
        "php": ">=7.1",
        "rudra/container" : "self.version"
    },
    "require-dev" : {
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload"    : {
        "psr-4": {
            "Rudra\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "config"            : {
        "optimize-autoloader" : true,
        "prefer-stable"       : true
    }
}
